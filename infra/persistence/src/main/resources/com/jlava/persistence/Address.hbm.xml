<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.jlava.model.Address" table="address" catalog="public">
        <id name="addressId" column="address_id" type="int">
            <generator class="identity"/>
        </id>
        <property name="street" column="street" type="string" length="50"/>
        <property name="barangay" column="barangay" type="string" length="50"/>
        <property name="municipality" column="municipality" type="string" length="50"/>
        <property name="zipCode" column="zipcode" type="int"/>
    </class>
</hibernate-mapping>